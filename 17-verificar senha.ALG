Algoritmo "semnome"
// ate usuario acertar a senha
Var
senha: inteiro


Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
escreva("Digite a sua senha: ")
leia(senha)

enquanto senha <> 2002 faca
         escreval("Senha inválida, tente novamente")
         leia(senha)
fimenquanto

escreval("Acesso liberado")

Fimalgoritmo